<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="84dp"
        android:layout_marginRight="84dp"
        android:layout_marginBottom="13dp"
        android:onClick="onClickProfe"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txtVwProfe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/assistant_extralight"
            android:gravity="center"
            android:text="Edgar Alejandro Tebar Nevarez"
            android:textSize="18sp"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/txtVwCalGen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/assistant_extralight"
            android:text="Calificacion general: 10"
            android:textColor="@color/black"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/txtVwCalAlu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/assistant_extralight"
            android:text="Calificacion de alumnos: 8"
            android:textColor="@color/black"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:background="@color/azul"
            android:textSize="1sp" />
    </LinearLayout>
</LinearLayout>